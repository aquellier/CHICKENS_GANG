<%= simple_form_for(@chickens_gang) do |f| %>

<div class="col-md-4 col-md-offset-4">
  <div class="form-inputs">
    <%= f.input :gang_name %>
    <%= f.input :breed %>
    <%= f.input :capacity %>
    <%= f.input :year_of_birth %>
    <%= f.input :price %>
    <%= f.input :photo %>
    <%#= f.input :photo_cache, as: :hidden %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
</div>

<% end %>

